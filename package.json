{
	"name": "@tic-tac-toe/source",
	"version": "0.2.2",
	"license": "MIT",
	"private": true,
	"repository": {
		"url": "https://github.com/tomcoolnl2/tic-tac-toe"
	},
	"engines": {
		"node": ">= 18.17.0"
	},
	"scripts": {
		"start:web": "nx serve web",
		"start:desktop": "yarn start:web & nx serve desktop",
		"start:api-auth": "nx serve api-auth",
		"build": "nx build web",
		"build:desktop": "nx run desktop:build",
		"test": "yarn test:web && yarn test:lib && yarn test:api",
		"test:coverage": "yarn test:lib:coverage && yarn test:api-auth:coverage",
		"test:web": "nx test web --collectCoverage --verbose",
		"test:lib": "yarn test:core && yarn test:ui",
		"test:lib:coverage": "yarn test:core:coverage && yarn test:ui:coverage",
		"test:api": "yarn test:api-auth",
		"test:core": "nx test core --collectCoverage --verbose",
		"test:core:coverage": "yarn test:core && npx serve ./coverage/core -p 2100",
		"test:ui": "nx test ui --collectCoverage --verbose",
		"test:ui:coverage": "yarn test:ui && npx serve ./coverage/ui -p 2110",
		"test:api-auth": "npx nx test api-auth --collectCoverage --verbose",
		"test:api-auth:coverage": "yarn test:api-auth && npx serve ./coverage/api-auth -p 2120",
		"e2e:web": "nx e2e web-e2e",
		"storybook": "nx run ui:storybook",
		"version:bump": "yarn version --no-git-tag-version",
		"version:update-changelog": "standard-version --release-as \"$npm_package_version\" --no-verify --commit-all",
		"format": "prettier --write 'apps/**/*.{ts,tsx,html,css,scss,json,md}'",
		"postinstall": "electron-builder install-app-deps"
	},
	"dependencies": {
		"@nestjs/common": "10.0.2",
		"@nestjs/core": "10.0.2",
		"@nestjs/platform-express": "10.0.2",
		"@swc/helpers": "0.5.2",
		"axios": "1.0.0",
		"classnames": "2.3.2",
		"conventional-recommended-bump": "9.0.0",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"reflect-metadata": "0.1.13",
		"rxjs": "7.8.0",
		"standard-version": "9.5.0",
		"tslib": "2.3.0"
	},
